<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team.radical.zoomove">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".GeneralActivity"
            android:theme="@style/AppTheme"
            android:label="@string/app_name"> ></activity>

        <activity android:name=".MuscleActivity"
            android:theme="@style/AppTheme"
            android:label="@string/app_name"> ></activity>

        <activity android:name=".RunActivity"
            android:theme="@style/AppTheme"
            android:label="@string/app_name"> ></activity>

        <activity android:name=".ExtrasActivity"
            android:theme="@style/AppTheme"
            android:label="@string/app_name"> ></activity>

        <activity android:name=".CharSelectActivity"
            android:theme="@style/AppTheme"
            android:label="@string/character_select"> ></activity>

        <activity android:name=".StatsActivity"
            android:theme="@style/AppTheme"
            android:label="@string/stats"> ></activity>

    </application>

</manifest>